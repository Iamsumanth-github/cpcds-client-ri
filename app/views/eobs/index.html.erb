<h1>Eob#index</h1>
<section class='container'> 
<section class='row col-12'> 
<div class='col-12'> 
<h3 class='float-left text-white'>Explanation of Benefits <%= @start_date %> - <%= @end_date %> </h3> 
<div class='btn-group float-right mt-2' role='group'> 
</div> 
</div> <table class='table table-dark'> 
<thead>
<tr> 
<th scope='col'></th> 
<th scope='col'>Date Created</th> 
<th scope='col'>Claim</th> 
<th scope='col'>Patient Name</th>
<th scope='col'>Provider Name</th> 
<th scope='col'>Facility Name</th> 
</tr> 
</thead>
<% @eobs.each do |eob| %> 
<tr> 
<th scope="row">
<td> <%= link_to sanitize(eob.created), eob_path(eob.id)%>
</th>
<td> <%= eob.claim_reference.last(6)%> </td> 
<td> <%= eob.claimpatient%> </td>
<td> <%= eob.careteam.values.map{ |c| c[:name]}.join(",")%> </td> 
<td> <%= eob.facility%> </td> 
 </tr> 
 <% end %> 
 </table> 
 <div class='col-12'> 
 <div class='btn-group float-right mt-2' role='group'> 
 </div> </div> 
 </section>
 </section>
